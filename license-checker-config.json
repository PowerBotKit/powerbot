{
  "ignore": [
    ".git",
    ".github/ISSUE_TEMPLATE",
    "coverage",
    "cypress",
    "**/*.log",
    ".idea",
    ".npmrc",
    ".husky/_",
    "*.DS_Store",
    "**/*.json",
    "**/*.svg",
    "**/*.png",
    "**/*.txt",
    "**/*.md",
    "**/.prettierrc",
    "**/.prettierignore",
    "**/OWNERS",
    "**/*.lock"
  ],
  "license": "LICENSE",
  "defaultFormat": {
    "eachLine": {
      "prepend": "# "
    },
    "append": ""
  },
  "licenseFormats": {
    "ts|js": {
      "eachLine": {
        "prepend": "// "
      },
      "append": ""
    }
  },
  "trailingWhitespace": "TRIM"
}
